{
  "name": "@upfluence/hyperevents",
  "version": "0.1.1",
  "private": true,
  "repository": "",
  "license": "MIT",
  "author": "",
  "workspaces": [
    "hyperevents",
    "test-app"
  ],
  "scripts": {
    "build": "yarn workspace @upfluence/hyperevents run build",
    "lint": "yarn workspaces run lint",
    "lint:fix": "yarn workspaces run lint:fix",
    "prepare": "yarn build",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "yarn workspace @upfluence/hyperevents run start",
    "start:test-app": "yarn workspace test-app run start",
    "test": "yarn workspaces run test"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "prettier": "^2.3.2"
  }
}
